<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape >
            <solid android:color="#ffffff" />

            <stroke
                android:width="4dp"
                android:color="#58caee"/>
            <corners
                android:radius="30dp" />

        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="#ffffff"/>
            <stroke
                android:width="3dp"
                android:color="#ffffff" />
            <corners
                android:radius="30dp" />

        </shape>
    </item>
</selector>