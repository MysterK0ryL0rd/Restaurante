<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/back">

    <include android:id="@+id/app_bar" layout="@layout/main_layout" android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/app_bar"
        android:id="@+id/tabla_otros"
        android:visibility="invisible" >

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="100dp"
            android:background="#f6b231"
            android:weightSum="1"
            android:id="@+id/linearLayout13"
            >

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Movimiento Consumo"
                android:id="@+id/textView102"
                android:layout_gravity="center"
                android:gravity="center"
                android:layout_weight="0.62"

                android:textSize="25dp"
                android:textColor="#ffffff"
                android:textStyle="bold" />
        </LinearLayout>

    </TableLayout>

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/app_bar"
        android:id="@+id/tabla_abc"
        android:visibility="invisible" >

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="100dp"
            android:background="#f6b231"
            android:weightSum="1"
            android:id="@+id/linearLayout10" >

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Clientes"
                android:id="@+id/textView99"
                android:layout_gravity="center"
                android:gravity="center"
                android:layout_weight="0.62"
                android:textSize="25dp"
                android:textColor="#ffffff"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="100dp"
            android:background="#10b2a4"
            android:id="@+id/linearLayout11" >

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Usuarios"
                android:id="@+id/textView100"
                android:layout_gravity="center"
                android:gravity="center"
                android:textSize="25dp"
                android:textColor="#ffffff"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="100dp"
            android:background="#ee6562"
            android:id="@+id/linearLayout12" >

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Articulos"
                android:id="@+id/textView101"
                android:gravity="center"
                android:textSize="25dp"
                android:textColor="#ffffff"
                android:textStyle="bold" />
        </LinearLayout>

    </TableLayout>

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/app_bar"
        android:id="@+id/tabla_reportes"
        android:visibility="invisible" >

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="100dp"
            android:background="#f6b231"
            android:weightSum="1"
            android:id="@+id/linearLayout4" >

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Estado de Resultados"
                android:id="@+id/textView94"
                android:layout_gravity="center"
                android:gravity="center"
                android:layout_weight="0.62"
                android:textSize="25dp"
                android:textColor="#ffffff"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="100dp"
            android:background="#10b2a4"
            android:id="@+id/linearLayout5" >

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Corte de Caja"
                android:id="@+id/textView95"
                android:layout_gravity="center"
                android:gravity="center"
                android:textSize="25dp"
                android:textColor="#ffffff"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="100dp"
            android:background="#ee6562"
            android:id="@+id/linearLayout7" >

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Totales por Serie"
                android:id="@+id/textView96"
                android:gravity="center"
                android:textSize="25dp"
                android:textColor="#ffffff"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="100dp"
            android:background="#31304a"
            android:id="@+id/linearLayout8" >

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Reporte de Existencias"
                android:id="@+id/textView97"
                android:gravity="center"
                android:textSize="25dp"
                android:textColor="#ffffff"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="100dp"
            android:background="#BF91C62D"
            android:id="@+id/linearLayout9" >

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Reporte Gerencial"
                android:id="@+id/textView98"
                android:gravity="center"
                android:textSize="25dp"
                android:textColor="#ffffff"
                android:textStyle="bold" />
        </LinearLayout>

    </TableLayout>

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/app_bar"
        android:id="@+id/tabla_documentos"
        android:visibility="invisible">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="100dp"
            android:background="#f6b231"
            android:weightSum="1"
            android:id="@+id/opc_remision">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Remision"
                android:id="@+id/textView90"
                android:layout_gravity="center"
                android:gravity="center"
                android:layout_weight="0.62"
                android:textSize="25dp"
                android:textColor="#ffffff"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="100dp"
            android:background="#10b2a4"
            android:id="@+id/opc_factura">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Factura"
                android:id="@+id/textView91"
                android:layout_gravity="center"
                android:gravity="center"
                android:textSize="25dp"
                android:textColor="#ffffff"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="100dp"
            android:background="#ee6562"
            android:id="@+id/opc_honorarios">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Honorarios"
                android:id="@+id/textView92"
                android:gravity="center"
                android:textSize="25dp"
                android:textColor="#ffffff"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="100dp"
            android:background="#31304a"
            android:id="@+id/opc_arrendamiento">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Arrendamientos"
                android:id="@+id/textView93"
                android:gravity="center"
                android:textSize="25dp"
                android:textColor="#ffffff"
                android:textStyle="bold" />
        </LinearLayout>
    </TableLayout>

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/app_bar"
        android:id="@+id/tabla_menu"
        android:visibility="visible">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="100dp"
            android:background="#f6b231"
            android:weightSum="1"
            android:id="@+id/opc_documentos">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="Documentos"
                android:drawableLeft="@drawable/clean_icon_0"
                android:layout_weight="0.62"
                android:textSize="25dp"
                android:textColor="#ffffff"
                android:textStyle="bold"
                android:gravity="center"
                android:layout_marginLeft="50dp" />

        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="100dp"
            android:background="#10b2a4"
            android:id="@+id/opc_reportes">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Reportes"
                android:drawableLeft="@drawable/clean_icon_0"
                android:layout_gravity="center"
                android:gravity="center"
                android:textSize="25dp"
                android:textColor="#ffffff"
                android:textStyle="bold"
                android:layout_marginLeft="50dp" />

        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="100dp"
            android:background="#ee6562"
            android:id="@+id/opc_abc">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="ABC Entidades"
                android:gravity="center"
                android:textSize="25dp"
                android:drawableLeft="@drawable/clean_icon_0"
                android:textColor="#ffffff"
                android:textStyle="bold"
                android:layout_marginLeft="50dp" />

        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="100dp"
            android:background="#31304a"
            android:id="@+id/opc_otros">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Otras Funciones"
                android:gravity="center"
                android:textSize="25dp"
                android:drawableLeft="@drawable/clean_icon_0"
                android:textColor="#ffffff"
                android:textStyle="bold"
                android:layout_marginLeft="50dp" />

        </LinearLayout>
    </TableLayout>


</RelativeLayout>
